extends layoutNavBar
//- comment testing
block content
  h1 #{title} 
  ul.list-group
  each reservation, i in reservations
    li.list-group-item
      p RoomType: #{reservation.roomstyle}
      p RoomNum: #{reservation.roomNum}
      //- a(href="/reservations/"+reservation._id)= reservation.roomstyle  
      p Reserved by: #{guestName}
      //-a(href="/reservations/"+reservation._id)= reservation.guest
      p Start Date: #{reservation.startDate}
      p End Date: #{reservation.endDate}
      p Check Status: #{reservation.checkInOutStatus}



      
